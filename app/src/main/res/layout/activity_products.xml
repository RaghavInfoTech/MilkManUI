<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:context=".ui.products.ProductsActivity">
    <androidx.cardview.widget.CardView
            android:layout_width="188dp"
            android:layout_height="129dp"
            app:cardElevation="10dp"
            app:cardCornerRadius="10dp"
            android:layout_margin="10dp"
            app:cardBackgroundColor="@color/yellow"
            app:cardMaxElevation="12dp"
            app:cardPreventCornerOverlap="true"
            app:cardUseCompatPadding="true" tools:layout_editor_absoluteY="506dp" tools:layout_editor_absoluteX="16dp"
            android:id="@+id/cardView5"/>
    <androidx.cardview.widget.CardView
            android:layout_width="196dp"
            android:layout_height="147dp"
            app:cardElevation="10dp"
            app:cardCornerRadius="10dp"
            android:layout_margin="10dp"
            app:cardBackgroundColor="@color/yellow"
            app:cardMaxElevation="12dp"
            app:cardPreventCornerOverlap="true"
            app:cardUseCompatPadding="true"
            android:id="@+id/cardView4"
            tools:layout_editor_absoluteY="334dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="199dp" android:layout_marginEnd="205dp"
            app:layout_constraintEnd_toEndOf="parent"/>
    <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="Food Products"
            style="@style/Widget.MaterialComponents.TextView"
    ></TextView>

    <androidx.cardview.widget.CardView
            android:layout_width="188dp"
            android:layout_height="129dp"
            app:cardElevation="10dp"
            app:cardCornerRadius="10dp"
            android:layout_margin="10dp"
            app:cardBackgroundColor="@color/yellow"
            app:cardMaxElevation="12dp"
            app:cardPreventCornerOverlap="true"
            app:cardUseCompatPadding="true" tools:layout_editor_absoluteY="205dp" tools:layout_editor_absoluteX="16dp"
            android:id="@+id/cardView6"/>

</androidx.constraintlayout.widget.ConstraintLayout>